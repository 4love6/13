using System.Text;
using System.Threading.Tasks;

namespace Word13
{
    internal class Manager : Employee
    {
        private int team_size;

        public int TeamSize { get { return team_size; } }

        public void Meeting()
        {
            Console.WriteLine($"Проводится собрание, количество людей - {team_size}");
        }
    }
}
