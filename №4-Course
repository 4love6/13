sing System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Word13_4
{
    internal class Course
    {
        private string descript; private string author; private double price;

        public string Descript {  get { return descript; } }
        public string Author { get { return author; } }
        public double Price { get { return price; } }
    }
}
